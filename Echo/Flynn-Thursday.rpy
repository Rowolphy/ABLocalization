translate Schinese flynnsaturday_f098983a_1:

    # f "\"You were there. Play it back to me, step by step.\""
    f "\"你曾经就在那里的。再给我回想一次，一步一步来。\""

# game/Flynn-Thursday.rpy:6978
translate Schinese flynnsaturday_530ae66c_1:

    # t "\"Flynn...\""
    t "\"Flynn......\""

# game/Flynn-Thursday.rpy:6979
translate Schinese flynnsaturday_d1704286_1:

