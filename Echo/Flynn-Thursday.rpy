# game/Flynn-Thursday.rpy:6938
translate Schinese flynnsaturday_abc1a553_1:

    # m "\"I know.\""
    m "\"我知道。\""

# game/Flynn-Thursday.rpy:6940
translate Schinese flynnsaturday_796f8b4f_1:

