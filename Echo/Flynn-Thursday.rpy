# game/Flynn-Thursday.rpy:2470
translate Schinese documents_done_picking_bca5d149:

    # m "\"Um.\""
    m "\"呃。\""

# game/Flynn-Thursday.rpy:2471
translate Schinese documents_done_picking_6dd90f8c:

    # "Ryan just grins, the red-light glistening upon the enamel of his fangs."
